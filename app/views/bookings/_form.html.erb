<%= simple_form_for [@car, booking] do |f| %>
  <% if @booking.errors.any? && @booking.errors.include?(:base) %>
    <p id="error-message"><%= @booking.errors[:base].first %></p>
  <% end %>
  <%= f.input :start_date,
              as: :string,
              input_html: { data: { controller: "datepicker" } } %>

  <%= f.input :end_date,
              as: :string,
              input_html: { data: { controller: "datepicker" } } %>
  <%= f.submit %>
<% end %>
